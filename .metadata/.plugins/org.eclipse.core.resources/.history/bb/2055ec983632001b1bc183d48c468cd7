/*
 * COMMCON.h
 *
 *  Created on: Nov 29, 2020
 *      Author: mazen
 *
 *   Description:
 *   Library for commonly used constant names in drivers
 *   like HIGH/LOW, ON/OFF, ENABLE/DISABLE, OK/NOK, ACK/NACK,...
 *
 */

#ifndef LIB_COMMCON_H_
#define LIB_COMMCON_H_


#define HIGH 1
#define LOW  0


#define INPUT  0
#define OUTPUT 1

#define ON  1
#define OFF 0




#endif /* LIB_COMMCON_H_ */
